<?xml version="1.0" encoding="utf-8"?>
<appwidget-provider xmlns:android="http://schemas.android.com/apk/res/android"
    android:autoAdvanceViewId="@+id/widget_stack"
    android:configure="com.ios.widget.ui.Activity.PhotoWidgetActivity"
    android:initialKeyguardLayout="@layout/layout_widget_photo_widget"
    android:initialLayout="@layout/layout_widget_photo_widget"
    android:minWidth="280dp"
    android:minHeight="90dp"
    android:minResizeWidth="40dp"
    android:minResizeHeight="40dp"
    android:targetCellWidth="4"
    android:targetCellHeight="2"
    android:previewImage="@drawable/ic_widget_preview_medium"
    android:resizeMode="none|horizontal|vertical"
    android:updatePeriodMillis="86400000"
    android:widgetCategory="home_screen" />
